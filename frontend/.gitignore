cat > frontend/.gitignore << 'EOF'
# Dependencies
node_modules/

# Build output
dist/
build/

# Environment variables
.env.local
.env.development.local
.env.test.local
.env.production.local

# Vite cache
.vite/

# Editor directories and files
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Coverage directory used by tools like istanbul
coverage/

# ESLint cache
.eslintcache
EOF
